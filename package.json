{
  "name": "webstory",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx run-many --target=test --all",
    "prepare": "husky install",
    "i18n": "ng extract-i18n webstory --format xlf --output-path apps/webstory/src/locale",
    "release": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.6",
    "@angular/cdk": "14.0.4",
    "@angular/common": "14.0.6",
    "@angular/compiler": "14.0.6",
    "@angular/core": "14.0.6",
    "@angular/forms": "14.0.6",
    "@angular/material": "14.0.4",
    "@angular/platform-browser": "14.0.6",
    "@angular/platform-browser-dynamic": "14.0.6",
    "@angular/router": "14.0.6",
    "@ng-select/ng-select": "^8.1.1",
    "@nrwl/angular": "14.4.2",
    "ngx-material-file-input": "^3.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.6",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "14.0.6",
    "@angular/language-service": "14.0.6",
    "@angular/localize": "14.0.6",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nrwl/cli": "14.4.2",
    "@nrwl/cypress": "14.4.2",
    "@nrwl/eslint-plugin-nx": "14.4.2",
    "@nrwl/jest": "14.4.2",
    "@nrwl/linter": "14.4.2",
    "@nrwl/workspace": "14.4.2",
    "@testing-library/angular": "^11.0.3",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "commitizen": "^4.2.4",
    "cypress": "^9.1.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "8.15.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-testing-library": "^5.0.4",
    "git-describe": "^4.1.0",
    "git-precommit-checks": "^3.0.7",
    "husky": "^7.0.4",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.2",
    "lint-staged": "^12.3.1",
    "prettier": "2.6.2",
    "pretty-quick": "^3.1.3",
    "standard-version": "^9.3.2",
    "ts-jest": "27.1.4",
    "typescript": "4.7.4",
    "nx": "14.4.2"
  },
  "git-precommit-checks": {
    "rules": [
      {
        "filter": "\\.ts$",
        "nonBlocking": "true",
        "message": "You’ve got leftover `console.log`",
        "regex": "console\\.log"
      },
      {
        "message": "You’ve got leftover conflict markers",
        "regex": "/^[<>|=]{4,}/m"
      },
      {
        "filter": "\\.ts$",
        "message": "You have unfinished devs",
        "nonBlocking": "true",
        "regex": "(?:FIXME|TODO)"
      }
    ],
    "display": {
      "notifications": true,
      "offendingContent": true,
      "rulesSummary": true,
      "shortStats": true,
      "verbose": true
    }
  },
  "lint-staged": {
    "*": [
      "nx affected:lint --fix --files",
      "nx format:write --files"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "standard-version": {
    "issueUrlFormat": "http://jira.afp.com/browse/{{id}}",
    "types": [
      {
        "type": "feat",
        "section": "Fonctionalités"
      },
      {
        "type": "fix",
        "section": "Corrections de bugs"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ],
    "skip": {
      "bump": true,
      "commit": true,
      "tag": true
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=16.10.0",
    "npm": ">= 8.1.2"
  }
}
